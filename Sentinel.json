{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "Redis Up",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "redis_up", "refId": "A" }],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "red", "value": null }, { "color": "green", "value": 1 } ] }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Uptime (hrs)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "(time() - redis_start_time_seconds) / 3600", "refId": "A" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": { "defaults": { "unit": "hours" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Connected Clients",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "redis_connected_clients", "refId": "A" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 }
    },
    {
      "type": "stat",
      "title": "CPU (process rate)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "rate(process_cpu_seconds_total[5m])", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "s/s" } },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Memory RSS",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "process_resident_memory_bytes", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "bytes" } },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Sentinel Masters",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "redis_sentinel_masters", "refId": "A" }],
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Master Status ($master)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "redis_sentinel_master_status{master_name=~\"$master\"}", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "NOT OK", "color": "red" }, "1": { "text": "OK", "color": "green" } } }
          ]
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 4 }
    },
    {
      "type": "stat",
      "title": "Quorum Check ($master)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "redis_sentinel_master_ckquorum_status{master_name=~\"$master\"}", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "FAIL", "color": "red" }, "1": { "text": "OK", "color": "green" } } }
          ]
        }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 4 }
    },
    {
      "type": "stat",
      "title": "Usable Sentinels ($master)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "redis_sentinel_master_ok_sentinels{master_name=~\"$master\"}", "refId": "A" }],
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 4 }
    },
    {
      "type": "stat",
      "title": "Slaves ($master)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "redis_sentinel_master_slaves{master_name=~\"$master\"}", "refId": "A" }],
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 4 }
    },
    {
      "type": "timeseries",
      "title": "Commands/s",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "rate(redis_commands_processed_total[5m])", "refId": "A" }],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
    },
    {
      "type": "timeseries",
      "title": "Errors/s (Error Replies)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "rate(redis_total_error_replies[5m])", "refId": "A" }],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
    },
    {
      "type": "timeseries",
      "title": "Connections Received/s",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "rate(redis_connections_received_total[5m])", "refId": "A" }],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
    },
    {
      "type": "timeseries",
      "title": "Network In/Out (bytes/s)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "rate(redis_net_input_bytes_total[5m])", "refId": "A", "legendFormat": "in" },
        { "expr": "rate(redis_net_output_bytes_total[5m])", "refId": "B", "legendFormat": "out" }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
    },
    {
      "type": "table",
      "title": "Master Settings ($master)",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "redis_sentinel_master_setting_down_after_milliseconds{master_name=~\"$master\"}", "refId": "A", "legendFormat": "down_after_ms" },
        { "expr": "redis_sentinel_master_setting_failover_timeout{master_name=~\"$master\"}", "refId": "B", "legendFormat": "failover_timeout_ms" },
        { "expr": "redis_sentinel_master_setting_parallel_syncs{master_name=~\"$master\"}", "refId": "C", "legendFormat": "parallel_syncs" }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 24 },
      "options": { "showHeader": true }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["redis", "sentinel"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "selected": true },
        "hide": 0
      },
      {
        "name": "master",
        "label": "master_name",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(redis_sentinel_master_status, master_name)",
        "includeAll": true,
        "refresh": 1,
        "multi": false,
        "current": { "text": "All", "value": ".*" },
        "regex": ""
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "",
  "title": "Redis Sentinel (Exporter)",
  "version": 1,
  "weekStart": ""
}
